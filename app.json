{
  "expo": {
    "name": "Stockrecorder",
    "description": "Expo app use for stock record",
    "slug": "Stockrecorder",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "android"
    ],
    "icon": "./assets/5453183.png",
    "privacy": "public",
    "splash": {
      "image": "./assets/5453183.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abdulgafar.stockrecorder"
    },
    "android": {
      "package": "com.abdulgafar.stockrecorder"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Stockrecorder"
    },
    "githubUrl": "https://github.com/Abdulgafar4/rtlfrontend"
  }
}
